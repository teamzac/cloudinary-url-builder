{
    "name": "teamzac/cloudinary",
    "description": "A fluent Cloudinary URL builder",
    "keywords": [
        "team-zac",
        "cloudinary"
    ],
    "homepage": "https://github.com/team-zac/cloudinary",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Chad Janicek",
            "email": "chad@zactax.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "cloudinary/cloudinary_php": "^1.18",
        "illuminate/support": "^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "TeamZac\\Cloudinary\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TeamZac\\Cloudinary\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "TeamZac\\Cloudinary\\ServiceProvider"
            ],
            "aliases": {
                "Cloudinary": "TeamZac\\Cloudinary\\Facades\\Cloudinary"
            }
        }
    }
}
